{
    "name": "Reddit",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "My First Chrome Extention",
    "icons": {
        "16": "images/reddit.png",
        "128": "images/reddit.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": ["scripts/popup.js"],
        "persistent": false
    },

    "permissions": ["<all_urls>"],

    "browser_action": {
    "default_icon": "images/reddit.png",
    "default_title": "Reddit",
    "default_popup": "popup.html"
  }
}
